<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:lawTreeItem="urn:import:lawscraper.client.ui.panels.lawtreeitem">
    <ui:style>
        .lawTable {
            margin-left: 8px;
            margin-bottom: 32px;

        }

        .controlBar {
            background-image: url("http://gwt-cx.com/serendipity/serendipity/gwtcx/skins/aqua/images/navigation_pane_header.png");
            background-repeat: repeat-x;
            height: 26px;
            border: 1px solid;
            bottom: 0;
            left: 0;
            right: 0;
            bottom: -32px;
        }

        .topControlBar {
            background-image: url("http://gwt-cx.com/serendipity/serendipity/gwtcx/skins/aqua/images/navigation_pane_header.png");
            background-repeat: repeat-x;
            height: 26px;
            border: 1px solid;
            top: 0;
            left: 0;
            right: 0;
        }

        .container {
            position: relative;
            border-left: 1px solid;
            border-right: 1px solid;
            border-bottom: 1px solid;
            height: 100%;
        }

        .bottomControlPanel {
            float: right;
        }

        .headerFont {
            color: #15428B;
            cursor: default;
            font-family: Tahoma, Verdana, sans-serif;
            font-size: 16px;
            font-weight: bold;
            position: absolute;
            left: 4px;
        }

        .searchBoxContainer {
            margin-top: 4px;
            margin-bottom: 4px;
            margin-left: 3%;
            width: 100%;
        }

        .searchBox {
            width: 90%;
        }

        .bodyContainer {

        }

        .alphabetButtons {
            margin-left: 8px;
            float: left;
            position: relative;
        }
    </ui:style>


