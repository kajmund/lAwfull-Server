<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .lawTable {
            height: 600px;
            width: 600px;
        }

        .part_paragraph {
            font-size: 20px;
            font-style: normal;
        }
        @external .part_paragraph;

        .part_section {
            font-size: 15px;
            margin-bottom: 16px;
            font-style: normal;

        }
        @external .part_section;

        .part_section_list_item {
            margin-left: 20px;
            font-size: 15px;
            margin-bottom: 10px;
            font-style: normal;
        }
        @external .part_section_list_item;

        .part_sub_heading {
            font-size: 18px;
            font-style: italic;
        }

        @external .part_sub_heading;

        .part_heading {
            font-size: 20px;
            font-style: normal;
        }
        @external .part_heading;


    </ui:style>


    <g:FlowPanel>
        <g:HTMLPanel ui:field="lawContainer" addStyleNames="{style.lawTable}"/>

    </g:FlowPanel>
</ui:UiBinder>